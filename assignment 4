#include <stdio.h>

int main() {
    int n,a[n],i,lowest;
    printf("enter nuber of numbers",n);
    scanf("%d",&n);
    printf("enter numbers");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
lowest=a[0];
    for(i=1;i<10;i++)
    {
        if(a[i]<lowest)
        lowest=a[i];
    }
printf("lowest is %d",lowest);
    return 0;
}
