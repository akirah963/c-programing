#include <stdio.h>

int main()
{
int n,a[n],i,largest;
    printf("enter nuber of numbers",n);
    scanf("%d",&n);
    printf("enter numbers");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
largest=a[0];
    for(i=1;i<10;i++)
    {
        if(a[i]>largest)
        largest=a[i];
    }
printf("largest is %d",largest);

    return 0;
}
